<h1>Edit User</h1>

<%= link_to  "Back", index_user_path %> |
<%= link_to  "Show", show_user_path(@user) %>
<%= form_with model: @user, url: update_user_path , local: true do |f| %>
  <table>
    <tr>
      <td><strong> Username: </strong></td>
      <td><%= f.text_field :username %></td>
    </tr>
    <tr>
      <td><strong> Password: </strong></td>
      <td><%= f.password_field :password %></td>
    </tr>
  </table>
  <%= f.submit %>
<% end %>

